# MongoDB Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/dbname?retryWrites=true&w=majority

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-2
AWS_S3_BUCKET=your-bucket-name

# Gumloop API Configuration
GUMLOOP_API_KEY=your_gumloop_api_key
GUMLOOP_USER_ID=your_gumloop_user_id
GUMLOOP_SAVED_ITEM_ID=your_gumloop_saved_item_id
GUMLOOP_BASE_URL=https://api.gumloop.com/api/v1/start_pipeline
GUMLOOP_RESULTS_URL=https://api.gumloop.com/api/v1/get_pl_run

# Use mock data for testing (set to 'true' to skip Gumloop API calls)
GUMLOOP_USE_MOCK=false
